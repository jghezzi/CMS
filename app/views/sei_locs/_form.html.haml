= form_for @sei_loc do |f|
  - if @sei_loc.errors.any?
    #error_explanation
      %h2= "#{pluralize(@sei_loc.errors.count, "error")} prohibited this sei_loc from being saved:"
      %ul
        - @sei_loc.errors.full_messages.each do |msg|
          %li= msg
  = f.label :name
  = f.text_field :name

  .actions
    = f.submit 'Save'
